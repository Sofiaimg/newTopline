<p id="notice"><%= notice %></p>

<h1>Clients</h1>

<table class="table table-striped table-bordered table-hover table-sm ">
  <thead>
    <tr>
      <th>Name</th>
      <th>RUT</th>
      <th>Address</th>
      <th>Country</th>
      <th>Phone</th>
      <th>link</th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.name %></td>
        <td><%= client.rut %></td>
        <td><%= client.address %></td>
        <td><%= client.country %></td>
        <td><%= client.phone %></td>
        <td><%= link_to 'Show', client %></td>
        </tr>
    <% end %>
  </tbody>
</table>

<br>
