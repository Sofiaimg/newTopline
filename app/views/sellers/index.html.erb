<p id="notice"><%= notice %></p>

<h1>Sellers</h1>

<table class="table table-striped table-bordered table-hover table-sm " id="myTable">
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Country</th>
      <th>Phone</th>
      <th>Email</th>
      <th>link</th>
      <th>Bank Details</th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.name%></td>
        <td><%= seller.address %></td>
        <td><%= seller.country %></td>
        <td><%= seller.phone %></td>
        <td><%= seller.email %></td>
        <td><%= link_to 'Show', seller %></td>
        <td><%= link_to 'Bank Details', seller_account_path(seller)%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
